{"ast":null,"code":"import \"core-js/modules/web.timers.js\";\nexport default {\n  methods: {\n    goBack: function goBack() {\n      // 获取当前距离顶部的距离\n      var scrollTop = document.documentElement.scrollTop;\n      console.log('scrollTop', scrollTop);\n      var steep = scrollTop / 2000;\n      var timer = setInterval(function () {\n        scrollTop -= steep;\n        steep = 200;\n        if (scrollTop <= 0) {\n          clearInterval(timer);\n        }\n        document.documentElement.scrollTop = scrollTop;\n      }, 10);\n    }\n  }\n};\nwindow.onscroll = function () {\n  var clientHeight = document.documentElement.clientHeight || document.body.clientHeight;\n  var scrollTop = document.documentElement.scrollTop;\n  var goTop = document.getElementById('goTop');\n  console.log('scrollTop', scrollTop);\n  console.log('clientHeight', clientHeight);\n  if (goTop != null) {\n    if (scrollTop > clientHeight) {\n      goTop.style.visibility = \"visible\";\n    } else {\n      goTop.style.visibility = \"hidden\";\n    }\n  }\n};","map":{"version":3,"names":["methods","goBack","scrollTop","document","documentElement","console","log","steep","timer","setInterval","clearInterval","window","onscroll","clientHeight","body","goTop","getElementById","style","visibility"],"sources":["G:\\site\\site\\src\\components\\Research\\Research.vue"],"sourcesContent":["<template>\r\n    <div class=\"reseach\">\r\n        <div class=\"content\">\r\n            <div class=\"research_nav\">\r\n                <ul>\r\n                    <li class=\"title_span\">Category</li>\r\n                    <li class=\"research_nav_name\"><RouterLink class=\"research_link\" to=\"/research/fund\">Fund</RouterLink></li>\r\n                    <li class=\"research_nav_name\"><RouterLink class=\"research_link\" to=\"/research/biology\">Research - Biology, genetics and genomics</RouterLink></li>\r\n                    <li class=\"research_nav_name\"><RouterLink class=\"research_link\" to=\"/research/machineLearning\">Research - Machine learning/datamining</RouterLink></li>\r\n                    <li class=\"research_nav_name\"><RouterLink class=\"research_link\" to=\"/research/interests\">Research interests - Heuristic search, planning and optimization</RouterLink></li>\r\n                </ul>\r\n            </div>\r\n            <router-view/>\r\n        </div>\r\n        <div class=\"goTop\" id = \"goTop\" @click=\"goBack\">\r\n            <div>\r\n                <p class=\"top_font\">Top</p>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n\r\n\r\n </template>\r\n\r\n <script>\r\n    export default {\r\n     methods:{\r\n\r\n        goBack() {\r\n            // 获取当前距离顶部的距离\r\n            let scrollTop = document.documentElement.scrollTop;\r\n            console.log('scrollTop',scrollTop);\r\n            let steep = scrollTop / 2000;\r\n            let timer = setInterval(() => {\r\n                scrollTop -= steep;\r\n                steep = 200;\r\n                if (scrollTop <= 0) {\r\n                clearInterval(timer);\r\n                }\r\n                document.documentElement.scrollTop = scrollTop;\r\n            }, 10);\r\n            }\r\n     }\r\n    }\r\n    window.onscroll = function(){\r\n        let clientHeight = document.documentElement.clientHeight || document.body.clientHeight;\r\n        let scrollTop = document.documentElement.scrollTop;\r\n        let goTop = document.getElementById('goTop');\r\n        console.log('scrollTop',scrollTop);\r\n        console.log('clientHeight',clientHeight);\r\n        if(goTop != null){\r\n            if (scrollTop > clientHeight) {\r\n                goTop.style.visibility = \"visible\";\r\n            } else {\r\n                goTop.style.visibility = \"hidden\";\r\n            }\r\n        }\r\n\r\n    }\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n.reseach{\r\n    width: 100%;\r\n    background-color: @intervalColor;\r\n    display: inline-block;\r\n}\r\n.research_nav{\r\n   line-height: 26px;\r\n   padding:30px 0 30px 7%;\r\n   display: inline-block;\r\n   width: 200px;\r\n   height: 380px;\r\n}\r\n.research_nav_name{\r\n    font-size: 18px;\r\n    padding-top: 30px;\r\n    .router-link-exact-active{\r\n                color: @highLightColor;\r\n                border-bottom: 1px solid @highLightColor;\r\n            }\r\n}\r\n.research_link{\r\n    color: @timeColor;\r\n    font-weight: bold;\r\n    &:hover{\r\n        color: @highLightColor;\r\n    }\r\n}\r\n\r\n.goTop{\r\n    scroll-behavior: smooth;\r\n    overflow: auto;\r\n    width: 40px;\r\n    height: 30px;\r\n    border: 1px solid @highLightColor;\r\n    border-radius: 2px;\r\n    margin-right: 10px;\r\n    position: sticky;\r\n    top: 30%;\r\n    left: 95%;\r\n    visibility: hidden;\r\n    cursor: pointer;\r\n}\r\n.top_font{\r\n    font-size: 18px;\r\n    padding-left: 10%;\r\n    padding-top: 10%;\r\n    color: @navColor;\r\n}\r\n\r\n</style>"],"mappings":";AA0BI,eAAe;EACdA,OAAO,EAAC;IAELC,MAAM,WAAAA,OAAA,EAAG;MACL;MACA,IAAIC,SAAQ,GAAIC,QAAQ,CAACC,eAAe,CAACF,SAAS;MAClDG,OAAO,CAACC,GAAG,CAAC,WAAW,EAACJ,SAAS,CAAC;MAClC,IAAIK,KAAI,GAAIL,SAAQ,GAAI,IAAI;MAC5B,IAAIM,KAAI,GAAIC,WAAW,CAAC,YAAM;QAC1BP,SAAQ,IAAKK,KAAK;QAClBA,KAAI,GAAI,GAAG;QACX,IAAIL,SAAQ,IAAK,CAAC,EAAE;UACpBQ,aAAa,CAACF,KAAK,CAAC;QACpB;QACAL,QAAQ,CAACC,eAAe,CAACF,SAAQ,GAAIA,SAAS;MAClD,CAAC,EAAE,EAAE,CAAC;IACN;EACP;AACD;AACAS,MAAM,CAACC,QAAO,GAAI,YAAU;EACxB,IAAIC,YAAW,GAAIV,QAAQ,CAACC,eAAe,CAACS,YAAW,IAAKV,QAAQ,CAACW,IAAI,CAACD,YAAY;EACtF,IAAIX,SAAQ,GAAIC,QAAQ,CAACC,eAAe,CAACF,SAAS;EAClD,IAAIa,KAAI,GAAIZ,QAAQ,CAACa,cAAc,CAAC,OAAO,CAAC;EAC5CX,OAAO,CAACC,GAAG,CAAC,WAAW,EAACJ,SAAS,CAAC;EAClCG,OAAO,CAACC,GAAG,CAAC,cAAc,EAACO,YAAY,CAAC;EACxC,IAAGE,KAAI,IAAK,IAAI,EAAC;IACb,IAAIb,SAAQ,GAAIW,YAAY,EAAE;MAC1BE,KAAK,CAACE,KAAK,CAACC,UAAS,GAAI,SAAS;IACtC,OAAO;MACHH,KAAK,CAACE,KAAK,CAACC,UAAS,GAAI,QAAQ;IACrC;EACJ;AAEJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}